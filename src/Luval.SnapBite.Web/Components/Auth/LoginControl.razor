@using Luval.AuthMate.Entities
@inject IHttpContextAccessor context

<div class="row">
    <div class="col">
        @if (!context.IsAuthenticated())
        {
            <a class="btn btn-secondary" href="/auth/google-login">Loging with Google</a>
        }
        else{
            <a class="btn btn-secondary" href="/auth/logout">Logout</a>
        }
    </div>
</div>

